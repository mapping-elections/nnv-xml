			<overview status="Version 0.5" modified="2012-06-15">
				<candidate_summary candidate_ref="1" vote_total="824" resp="Philip Lampi"/>
				<candidate_summary candidate_ref="2" vote_total="801" resp="Philip Lampi"/>
